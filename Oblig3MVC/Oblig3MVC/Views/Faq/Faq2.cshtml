@model List<Oblig3MVC.Models.Faq>

@{
    ViewBag.Title = "Faq1";
}

<!DOCTYPE html>
<html>
<head>
    <title>ListKategorier</title>
</head>
<body>
    <div>
        <br />
        <br />
        <h4>Faqs:</h4><br />
        <br />

        <table>
            @foreach (var Faq in Model)
            {
                if (Faq.id == 4)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq21")
                        <td />

                    </tr>
                }
                else if (Faq.id == 5)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq22")
                        <td />

                    </tr>
                }
                else if (Faq.id == 6)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq23")
                        <td />

                    </tr>
                }


            }
        </table>

        <br />@Html.ActionLink("Tilbake", "Faqs")

    </div>
</body>
</html>

