@model List<Oblig3MVC.Models.Faq>

@{
    ViewBag.Title = "Faq1";
}

<!DOCTYPE html>
<html>
<head>
    <title>ListKategorier</title>
</head>
<body>
    <div>
        <br />
        <br />
        <h4>Faqs:</h4><br />
        <br />

        <table>
            @foreach (var Faq in Model)
            {
                if (Faq.id == 7)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq31")
                        <td />

                    </tr>
                }
                else if (Faq.id == 8)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq32")
                        <td />

                    </tr>
                }
                else if (Faq.id == 9)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq33")
                        <td />

                    </tr>
                }


            }
        </table>
        
        <br />@Html.ActionLink("Tilbake", "Faqs")
              
</div>
</body>
</html>

