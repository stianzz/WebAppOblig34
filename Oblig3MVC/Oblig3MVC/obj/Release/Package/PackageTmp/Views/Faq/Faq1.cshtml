@model List<Oblig3MVC.Models.Faq>

@{
    ViewBag.Title = "Faq1";
}

<!DOCTYPE html>
<html>
<head>
    <title>ListKategorier</title>
</head>
<body>
    <div>
        <br />
        <br />
        <h4>Faqs:</h4><br />
        <br />
        
        <table>
            @foreach (var Faq in Model)
            {
                if (Faq.id == 1)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq11")
                        <td />

                    </tr>
                }
                else if (Faq.id == 2)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq12")
                        <td />

                    </tr>
                }
                else if (Faq.id == 3)
                {
                    <tr>

                        <td>

                            @Html.ActionLink(Faq.problem, "Faq13")
                        <td />

                    </tr>
                }


            }
        </table>

        <br />@Html.ActionLink("Tilbake", "Faqs")

</div>
</body>
</html>
